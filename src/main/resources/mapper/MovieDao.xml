<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kevin.mirs.dao.MovieDao">

    <select id="getMovieByMovieId" resultType="com.kevin.mirs.entity.Movie">
        SELECT * FROM mirs_movie WHERE id = #{id}
    </select>
    <select id="getAllMovie" resultType="com.kevin.mirs.entity.Movie">
        SELECT * FROM mirs_movie;
    </select>
</mapper>